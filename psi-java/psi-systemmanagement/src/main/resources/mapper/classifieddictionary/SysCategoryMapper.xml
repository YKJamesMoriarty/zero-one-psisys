<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zeroone.star.systemmanagement.mapper.classifieddictionary.SysCategoryMapper">
    <select id="selectNodeOrderByCreateTime" resultType="com.zeroone.star.systemmanagement.entity.classifieddictionary.SysCategory">
        select * from sys_category where pid = #{pid} order by create_time DESC
    </select>
</mapper>